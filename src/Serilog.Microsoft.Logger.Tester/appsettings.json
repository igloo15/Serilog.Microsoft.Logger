{
  "Logging": {
    "File": {
      "LogLevel": {
        "Default": "Warning"
      },
      "Shared": true,
      "Template": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message} ({EventId:x8}){NewLine}{Exception}",
      "FlushInterval": 2,
      "PathFormat": "Logs/MyApp.log",
      "FileCountLimit": 31,
      "IncludeScopes": true,
      "IsJson": true
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "IncludeScopes": true,
      "Template": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
    }
  }
}
